//Buzzer

void setup () {
  Serial.begin(9600);
  pinMode(8,OUTPUT);

  buzz();
}

void loop() {
}
  void buzz() {
  for(int i=0; i<5; i++){
    digitalWrite(8, HIGH); // sets the LED on
    delay(1000);                // waits for a second
    digitalWrite(8, LOW);  // sets the LED off
    delay(1000);                // waits for a second
  }
}
